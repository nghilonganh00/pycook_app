#:kivy 2.2.1
<Account>:
    md_bg_color: rgba(15,15,15,255)
    MDScrollView:
        bar_width: 0
        MDBoxLayout:
            orientation: 'vertical'
            size_hint_y: None  
            height: self.minimum_height
            MDFloatLayout:
                size_hint: None, None
                size: 360, 260
                md_bg_color: rgba(36, 36, 36, 255)
                Image:
                    source: 'assets/image/avatar/mai_ngo.png'
                    pos_hint: {"center_x": 0.14, "center_y": 0.58}
                    size_hint: .5, .5
                
                MDLabel:
                    text: root.fullname
                    pos_hint: {"center_x": 0.8, "center_y": 0.64}
                    font_name: "BPoppins"
                    font_size: 20
                    color: rgba(255, 255, 255, 255)
                MDLabel:
                    text: root.pycookID
                    pos_hint: {"center_x": 0.8, "center_y": 0.54}
                    font_name: "RRoboto"
                    font_size: 16
                    color: rgba(248,245,242,255)
                MDLabel:
                    text: "0 Bạn bếp"
                    pos_hint: {"center_x": 0.56, "center_y": 0.32}
                    font_name: "RRoboto"
                    font_size: 14
                    color: rgba(248,245,242,255)
                MDLabel:
                    text: "0 Người quan tâm"
                    font_name: "RRoboto"
                    pos_hint: {"center_x": 0.86, "center_y": 0.32}
                    font_size: 14
                    color: rgba(248,245,242,255)
                MDFlatButton:
                    text: "Sửa thông tin cá nhân"
                    size_hint_x: 0.9
                    pos_hint: {"center_x": 0.5, "center_y": 0.14}
                    font_name: "RRoboto"
                    theme_text_color: "Custom"
                    text_color: rgba(248,245,242,255)
                    line_width: 1.1
                    border_radius: [20] 
                    line_color: rgba(147, 146, 144, 255)
            MDBoxLayout:
                size_hint_y: None
                height: self.minimum_height + 10
                MDBoxLayout:
                    orientation: 'vertical'
                    size_hint_y: None
                    height: self.minimum_height
                    padding: (10, 20, 10, 20)
                    spacing: 10
                    md_bg_color: rgba(36, 36, 36, 255)
                    MDLabel:
                        id: my_food_total
                        text: 'Món ăn của tôi'
                        font_size: 20
                        font_name: 'BRoboto'
                        color: rgba(255, 255, 255, 255)
                        size_hint_y: None
                        height: self.texture_size[1]
                    MDScrollView:
                        # do_scroll_y: False
                        size_hint_y: None
                        height: 240
                        bar_width: 0
                        MDBoxLayout:
                            id: my_food
                            orientation: 'horizontal'
                            size_hint: None, None
                            # size_hint_x: 6
                            width: self.minimum_width
                            height: self.minimum_height
                            spacing: 10
